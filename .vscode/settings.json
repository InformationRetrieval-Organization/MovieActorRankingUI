{
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.enabled": false,
  "appService.zipIgnorePattern": [
    "node_modules{,/**}",
    ".vscode{,/**}"
  ],
  "appService.deploySubpath": ".",
